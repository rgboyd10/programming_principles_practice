#include <iostream>
using namespace std;

int main()
{
  //declare variables
  vector <string> nums = {"zero", "one", "two", "three", "four", "five", "six", "seven", "eight", "nine", "ten"};
  int userin;
  
  //prompt user for a number between 0-10
  cout << "Please enter a number between 0-10";
  cin >> userin;
  //find the match
  switch(userin)
    {
    case '0': cout << nums[0];
	break;
    case '1': cout << nums[1];
	break;
    case '2': cout << nums[2];
	break;
    case '3': cout << nums[3];
	break;
    case '4': cout << nums[4];
      break;
    case '5': cout << nums[5];
	break;
    case '6': cout << nums[6];
	break;
    case '7': cout << nums[7];
	break;
    case '8': cout << nums[8];
	break;
    case '9': cout << nums[9];
	break;
    case '10': cout << nums[10];
	break;
    }

  //print the match
}
